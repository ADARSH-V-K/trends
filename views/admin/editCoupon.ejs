<%- include("../layouts/header.ejs") %>
<section>
  <div class="container mt-4">
     <div class="row">
       
      <div class="col-md-6">
          <h2 class="text-center">Update Coupon</h2>
          <form action="" method="post"   >
              
              
              <label for="" class="mt-3">Coupon Code</label>
              <input type="text"name="CouponCode"class="form-control"  value="<%= couponData.CouponCode %>" >
              <input type="hidden" name="id" value="<%= couponData._id %>">
  
              <label for="" class="mt-4">Discount Type</label>
              <input type="text"name="discountType"class="form-control" value="<%=couponData.discountType  %>" >
  
              <label for="" class="mt-4"> Discount Amount</label>
              <input type="text"name=" discountAmount"class="form-control" value="<%= couponData.discountAmount %>" >
              <label for="" class="mt-4"> Max Discount Amount</label>
              <input type="text"name="maxDiscountAmount"class="form-control" value="<%= couponData.maxDiscountAmount %>" >
              <label for="" class="mt-4"> Min Purchase</label>
              <input type="text"name="minPurchase"class="form-control" value="<%= couponData. minPurchase %>" >
              <input type="submit" value="Submit" class="btn btn-success mt-4">
          </form>
      </div>
     </div>   
  </div>
  </section>
  <%- include("../layouts/footer.ejs") %>
