	<!-- Shoping Cart -->
	<% if(data.wishlist.length>0)
	{%>

		<div class="container">
			<div class="row">
				<div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
					<div class="m-l-25 m-r--38 m-lr-0-xl">
						<div class="wrap-table-shopping-cart">
							<table class="table-shopping-cart">
								<tr class="table_head">
									<th class="column-1">Product image</th>
									<th class="column-2">Product name</th>
									<th class="column-3">Price</th>
									<th class="column-4">Action</th>
								</tr>
								
                                <% 
                                if(data.wishlist.length>0){
                                    for(let i=0;i<data.wishlist.length;i++)
                                    {
                                        %>
								<tr class="table_row">
									<td class="column-1">
										<div class="how-itemcart1">
											<img src="/trendimages/<%=data.wishlist[i].productId.image[0]%>" alt="IMG">
										</div>
									</td>
									<td class="column-2"><%=data.wishlist[i].productId.name%></td>
									<td class="column-3"><%=data.wishlist[i].productId.price%></td>
									<td class="column-4">   <a href="/addCart?id=<%=data.wishlist[i].productId._id%>" class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
										<button type="submit" class="btn btn-primary btn-sm">Add to Cart</button>
									</a></td>
									<td class="column-5">   <a href="/removeWishlist?id=<%=data.wishlist[i].productId._id%>" class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
										<button type="submit" class="btn btn-primary btn-sm">remove</button>
									</a></td>
									</td>
								
								</tr>
                                <%}
                            }
						%>
							

							</table>
						
						</div>
                        

						<div class="flex-w flex-sb-m bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm">
							<div class="flex-w flex-m m-r-20 m-tb-5">
								
									
								
							</div>

						
						</div>
					</div>
				</div>
						<div class="flex-w flex-t p-t-27 p-b-33">
							<div class="size-208">
								<span class="mtext-101 cl2">
								
								</span>
							</div>

							<div class="size-209 p-t-1">
								<span class="mtext-110 cl2">
								
								</span>
							</div>
						</div>

					
					</div>
				</div>
			</div>
		</div>
		<%}
